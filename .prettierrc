{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "plugins": [
        "prettier-plugin-import-sort",
        "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "./resources/css/app.css"
}